{
  "name": "Toggle",
  "notes": "",
  "template": "Toggle.html",
  "class": "ms-Toggle",
  "branches": [
      {
        "name": "Text above",
        "class": "ms-Toggle--textAbove",
        "default": true,
        "branches": [
            {
              "name": "Enabled",
              "default": true
            },
            {
              "name": "Disabled",
              "class": "is-disabled"
            }
        ]
      },
      {
        "name": "Text left",
        "class": "ms-Toggle--textLeft",
        "branches": [
            {
              "name": "Enabled",
              "default": true
            },
            {
              "name": "Disabled",
              "class": "is-disabled"
            }
        ]
      }
  ]
}
